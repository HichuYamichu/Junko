    "zucc": "^0.1.2"
  },
  "eslintConfig": {
    "extends": "marine/node",
    "rules": {
      "brace-style": "error",
      "no-useless-constructor": "off",
      "function-paren-newline": "off",
      "@typescript-eslint/indent": [
